{
  "hosting": {
    "site": "devtools-wsgrok",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/200.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(html|js|css|png|jpg|jpeg|svg|webp|woff|woff2|ttf|ico)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  }
}
